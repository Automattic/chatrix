        if ( el ) {
            el.addEventListener( 'click', function( e ) {
                var $welcome = jQuery( '.make-welcome' ),
                    $toggle  = $welcome.find( '#make-welcome-toggle'),
                    $content = $welcome.find( '#make-welcome-content'),
