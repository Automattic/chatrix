        [{"widgetID":"o2_extend_resolved_posts_unresolved_posts-2","state":"unresolved","filterTags":["editor-requests"],"found":15},{"widgetID":"o2_extend_resolved_posts_unresolved_posts-3","state":"unresolved","filterTags":["locale-requests"],"found":21},{"widgetID":"o2_extend_resolved_posts_unresolved_posts-4","state":"unresolved","filterTags":["-locale-requests","-editor-requests"],"found":3}]
