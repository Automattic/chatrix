{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 2,
  "name": "automattic/chatrix",
  "version": "0.3.9",
  "title": "Chatrix",
  "category": "embed",
  "icon": "format-chat",
  "description": "Matrix client",
  "attributes": {
    "defaultHomeserver": {
      "type": "string",
      "default": ""
    },
    "roomId": {
      "type": "string",
      "default": ""
    },
    "height": {
      "type": "object",
      "default": {
        "value": 600,
        "unit": "px"
      }
    },
    "border": {
      "type": "object",
      "default": {
        "width": {
          "value": 1,
          "unit": "px"
        },
        "radius": {
          "value": 10,
          "unit": "px"
        },
        "style": "solid",
        "color": "black"
      }
    }
  },
  "supports": {
    "html": false
  },
  "textdomain": "chatrix",
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "style": "file:./style-index.css"
}
