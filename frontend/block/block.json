{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 2,
  "name": "automattic/chatrix",
  "version": "0.3.9",
  "title": "Chatrix",
  "category": "embed",
  "icon": "format-chat",
  "description": "Matrix client",
  "attributes": {
    "defaultHomeserver": {
      "type": "string",
      "source": "attribute",
      "attribute": "data-default-homeserver",
      "selector": "span[data-default-homeserver]",
      "default": ""
    },
    "roomId": {
      "type": "string",
      "source": "attribute",
      "attribute": "data-room-id",
      "selector": "span[data-room-id]",
      "default": ""
    },
    "height": {
      "type": "object",
      "source": "attribute",
      "attribute": "data-height",
      "selector": "span[data-height]",
      "default": {
        "value": 600,
        "unit": "px"
      }
    },
    "borderWidth": {
      "type": "object",
      "source": "attribute",
      "attribute": "data-border-width",
      "selector": "span[data-border-width]",
      "default": {
        "value": 1,
        "unit": "px"
      }
    },
    "borderRadius": {
      "type": "object",
      "source": "attribute",
      "attribute": "data-border-radius",
      "selector": "span[data-border-radius]",
      "default": {
        "value": 10,
        "unit": "px"
      }
    },
    "borderStyle": {
      "type": "string",
      "source": "attribute",
      "attribute": "data-border-style",
      "selector": "span[data-border-style]",
      "default": "solid"
    },
    "borderColor": {
      "type": "string",
      "source": "attribute",
      "attribute": "data-border-color",
      "selector": "span[data-border-color]",
      "default": "black"
    }
  },
  "supports": {
    "html": false
  },
  "textdomain": "chatrix",
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "viewScript": "file:./view.js",
  "style": "file:./style-index.css"
}
